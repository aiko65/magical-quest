	mes "セーブしますか?"
	button "はい",*game_save_real
	button "いいえ",*game_save_cansel
	stop
*game_save_real
	cls
	log "セーブ開始"
	dialog "svm",17,"マジカルクエストセーブファイル"
	if stat != 1 {
		mes "セーブに失敗しました"
		button "戻る",*game_save
		stop
