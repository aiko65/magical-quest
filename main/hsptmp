*prog_macro
//#define log prog_debuglog = prog_debuglog + "\n" +gettime(4) + ":" + gettime(5) + ":" + gettime(6) + "." + gettime(7) + "	: " +
#define log logmes "" + gettime(4) + ":" + gettime(5) + ":" + gettime(6) + "." + gettime(7) + " 	: " +
#define pday gettime(0) + "/"+ gettime(1) + "/" + gettime(3) + " "
#define ptime gettime(4) + ":" + gettime(5) + ":" + gettime(6) + "." + gettime(7)
*prog_start
	log "初期化開始"
	onerror goto *prog_error
	randomize
*prog_varreset
	mes "初期化中..."
	common_myhp = 0
	common_myexp = 0
	common_mylevel = 0
	battle_attackamount = 0
	enemy_hp = 0
	battle_remainexp = 0
	battle_oldlevel = 0
	battle_myexpadd = 0
	game_savedatep = 0
	game_load_status = 0
	battle_currentenemyID = 0
	
	i = 0
	j = 0
	k = 0
	
	prog_errc = ""
	prog_debug1 = ""
	prog_debug2 = ""
	wtext = ""
	wtext2 = ""
	prog_debuglog = ""
	enemydate = ""
	enemy_name = ""
	log "変数初期化完了"
*prog_dim
	dim item_have
	sdim enemies_name
	dim enemiesHP
	dim enemiesimg
	dim enemieshpr
	dim enemiesAPr
	dim enemiesAP
	dim enemiesExp
	dim enemiesExpr
	
	log "配列初期化完了"
*game_title
	cls
	#ifdef _debug
	goto *game_start_nodate
	#endif
	mes "マジカルクエスト"
	button "はじめから",*game_start_nodate
	button "つづきから",*game_start_continue
	button "終わる",*game_end
	log "タイトル表示"
	stop
*game_end
	end
*game_start_nodate
	log "はじめから開始"
	cls
	mes "セーブデータ作成中..."
	game_savedatep = "def:none"
	common_myhp = 100
	;wait 30
	goto *game_reset

*game_start_continue
	log "続きから開始"
	cls
	log "読み込みダイアログ表示"
	dialog "svm",16,"マジカルクエストセーブファイル"
	wtext = ""
	notesel wtext
	game_savedatep = refstr
	noteload game_savedatep
	noteget i,0
	if i != 1 {
		prog_debug1 = "セーブデータのバージョンが異なります。"
		prog_debug2 = "セーブデータバージョン変換ツールを使用してバージョンを1にしてください"
		prog_errc = "E160"
		goto *prog_error
	}
	noteget common_myhp,0
	noteget commoon_myexp,1
	noteget item_have(0),2
	common_mylevel = common_myexp/100
	log "読み込み終了"
	goto *game_reset

*game_reset
	log "敵情報解析準備"
	mes "敵の情報を読み込んでいます..."
	enemydate = ""
	notesel enemydate
	noteload "enemy.txt"
	mes "敵の情報を解析しています..."
	i = 0		//現在の行
	j = 0		//現在の敵ID
*game_date_analysis
	log "敵情報ファイル解析開始"
	repeat
		k = 0					//読み込みフラグ
		log i + "行目読み込み開始"
		noteget wtext2,i
		if i >= noteinfo(0) {
			break
		}
		if wtext2 == "start" {
			if game_load_status == 0 {
				game_load_status = 1
				i ++
				j ++
			}else{
				prog_debug1 = "敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E100です。製作者に報告してください。"
				prog_errc = "E100"
				goto *prog_error
			}
			k = 1
		}
		if wtext2 == "End" {
			if game_load_status == 0 {
				prog_debug1 = "敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E150です。製作者に報告してください。"
				prog_errc = "E150"
				goto *prog_error
			}else{
				game_load_status = 0
				i ++
				k = 1
				log "敵情報解析終了"
			}
		}
		if wtext2 == "comment" {
			log "コメントを認識"
			i += 2
			k = 1
		}
		if wtext2 == "Name" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "敵の名前を認識:" + wtext
				enemies_name(j) = wtext
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E101です。製作者に報告してください。"
				prog_errc = "E101"
				goto *prog_error
			}
			
		}
		if wtext2 == "ImagePos" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "画像位置を認識"
				enemiesimg(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E102です。製作者に報告してください。"
				prog_errc = "E102"
				goto *prog_error
			}

		}
		if wtext2 == "HP" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "HP情報を認識"
				enemieshp(j) = int(wtext)
			i += 2
			k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E103です。製作者に報告してください。"
				prog_errc = "E103"
				goto *prog_error
			}
			
		}
		if wtext2 == "HPRange" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "HPRangeを取得"
				enemieshpr(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E104です。製作者に報告してください。"
				prog_errc = "E104"
				goto *prog_error
			}
			
		}
		if wtext2 == "AttackPower" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "攻撃力を認識"
				enemiesAP(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E105です。製作者に報告してください。"
				prog_errc = "E105"
				goto *prog_error
			}
		}
		if wtext2 == "AtpRange" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "atprangeを認識"
				enemiesAPr(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E106です。製作者に報告してください。"
				prog_errc = "E106"
				goto *prog_error
			}
			
		}
		if wtext2 == "GetExp" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "取得経験値量を認識"
				enemiesExp(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E107です。製作者に報告してください。"
				prog_errc = "E107"
				goto *prog_error
			}
		}
		if wtext2 == "GetExpRange" {
			noteget wtext,i+1
			if game_load_status == 1 {
				log "取得経験値量rangeを認識"
				enemiesExpr(j) = int(wtext)
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
				prog_debug2 = "エラー番号は@E108です。製作者に報告してください。"
				prog_errc = "E108"
				goto *prog_error
			}
			
		}
		if wtext2 == "version" {
			noteget wtext,i+1
			if wtext == "1" {
				log "バージョンを認識"
				i += 2
				k = 1
			}else{
				prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが未対応のバージョンです。"
				prog_debug2 = "実行ファイルと同一の世代の敵情報ファイルをご利用ください。"
				prog_errc = "E130"
				goto *prog_error
			}
		}
		if k == 0 {
			prog_debug1 = "" + i + "行目の、敵ID" + j+ "の敵の情報ファイルが破損しています。"
			prog_debug2 = "エラー番号は@E140です。製作者に報告してください。"
			prog_errc = "E140"
			goto *prog_error
		}
		await
	loop
	wtext = ""
	wtext2 = ""
	
	
	goto *base_top
*end_load______________________________________________________________________________________________________________________________________________________________
*base_top
	screen 0,1080,720
	#ifdef _debug
	goto *battle_start
	#endif
	cls
	log "本拠地"
	mes "本拠地です"
	mes "ステージを選択してください"
	button "簡単",*battle_easy
	button "普通",*battle_mid
	button "難し",*battle_hard
	button gosub "保存",*game_save
	stop
*battle_easy
	stage = 0
	log "簡単ステージに移動"
	goto *game_field_start
*battle_mid
	stage = 1
	log "普通ステージに移動"
	goto *game_field_start
*battle_hard
	stage = 2
	log "難ステージに移動"
	goto *game_field_start
*game_field_start
	cls
	log "フィールド一回目"
	mes "フィールド"
	
	button "歩く",*game_field_check
	stop
*game_field_check
	log "フィールド"
	cls
	i = rnd(10)
	if stage == 0 and i <= 1 {
		battle_currentenemyID = 1
		goto *battle_start
	}
	
	if stage == 1 and i <= 1 {
		battle_currentenemyID = 1
		goto *battle_start
	}
	if stage == 1 and i > 1 and i <= 2 {
		battle_currentenemyID = 2
		goto *battle_start
	}
	
	if stage == 2 and i <= 1 {
		battle_currentenemyID = 2
		goto *battle_start
	}
	if stage == 2 and i > 1 and i <= 3 {
		battle_currentenemyID = 3
		goto *battle_start
	}
	if stage == 3 and i <= 3 {
		battle_currentenemyID = 3
		goto *battle_start
	}
	if i == 9 {
		goto *stage_clear
	}
	mes "フィールド"
	button "歩く",*game_field_check
	stop
*battle_start
	cls
	log "戦闘開始"
	wait 50
dim battle_timedbg,10
battle_scene = 0
/*
0:行動選択
1:行動中
2:逃げる
3:敗北
4:勝利
*/
repeat
redraw 0
color 255,255,255
boxf
battle_timedbg(0) = gettime(5)*60000+gettime(6)*1000+gettime(7)
switch battle_scene
	case 0
		gosub *battle_chose
	swbreak
	case 1
		gosub *battle_attacking
	swbreak
	case 2
		gosub *battle_exit
	swbreak
	case 3
		gosub *battle_lose
	swbreak
	case 4
		gosub *battle_win
	swbreak
swend
battle_timedbg(1) = gettime(5)*60000+gettime(6)*1000+gettime(7)
flamet = battle_timedbg(1)-battle_timedbg(0)
if cnt\100 = 0 : log "★全体バトル処理時間★" + flamet
redraw 1
await 16
loop
*battle_chose
	color
	pos 0,0
	font msgothic,16
	mes "どこどこ村・どこそこ"

	getkey isclick,1

	line 100,300,380,300
	line 380,300,380,400
	line 380,400,100,400
	line 100,400,100,300
	color 220,220,220
	boxf 102,302,378,398
	color 
	pos 140,300
	font msgothic,100
	mes "殴る"
	if mousex > 100 and mousey > 300 and mousex < 380 and mousey < 400{
		color 255,0,0
		boxf 102,302,378,398
		color 
		pos 140,300
		font msgothic,100
		mes "殴る"
		if isclick = 1{
			dowhat = 0
			battle_scene = 1
		}
	}

	color
	line 400,300,680,300
	line 680,300,680,400
	line 680,400,400,400
	line 400,400,400,300
	color 220,220,220
	boxf 402,302,678,398
	color 
	pos 440,300
	font msgothic,100
	mes "自殺"
	if mousex > 400 and mousey > 300 and mousex < 680 and mousey < 400{
		color 255,0,0
		boxf 402,302,678,398
		color 
		pos 440,300
		font msgothic,100
		mes "自殺"
		if isclick = 1{
			battle_scene = 3
		}
	}
	
	line 700,300,980,300
	line 980,300,980,400
	line 980,400,700,400
	line 700,400,700,300
	color 220,220,220
	boxf 702,302,978,398
	color 
	pos 740,300
	font msgothic,100
	mes "逃走"
	if mousex > 700 and mousey > 300 and mousex < 980 and mousey < 400{
		color 255,0,0
		boxf 702,302,978,398
		color 
		pos 740,300
		font msgothic,100
		mes "逃走"
		if isclick = 1{
			battle_scene = 2
		}
	}
	return
*battle_attacking
	color
	pos 0,0
	font msgothic,16
	mes "どこどこ村・どこそこ"

	getkey isclick,1

	pos 0,0
	font msgothic,50
	mes "うがー"
	
	
	line 100,300,380,300
	line 380,300,380,400
	line 380,400,100,400
	line 100,400,100,300
	color 220,220,220
	boxf 102,302,378,398
	color 
	pos 140,300
	font msgothic,100
	mes "OK"
	if mousex > 100 and mousey > 300 and mousex < 380 and mousey < 400{
		color 255,0,0
		boxf 102,302,378,398
		color 
		pos 140,300
		font msgothic,100
		mes "OK"
		if isclick = 1{
			dowhat = 0
			battle_scene = 0
		}
	}

	color
	line 400,300,680,300
	line 680,300,680,400
	line 680,400,400,400
	line 400,400,400,300
	color 220,220,220
	boxf 402,302,678,398
	color 
	pos 440,300
	font msgothic,100
	mes "なぜ勝たない"
	if mousex > 400 and mousey > 300 and mousex < 680 and mousey < 400{
		color 255,0,0
		boxf 402,302,678,398
		color 
		pos 440,300
		font msgothic,100
		mes "なぜ勝たない"
		if isclick = 1{
			battle_scene = 4
		}
	}
	return
*battle_exit
	cls
	log "逃走"
